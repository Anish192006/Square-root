#include <stdio.h>
#include <math.h>  // Required for sqrt()

int main() {
    double number, result;

    // Input from user
    printf("Enter a number: ");
    scanf("%lf", &number);

    // Check if number is non-negative
    if (number < 0) {
        printf("Square root of a negative number is not real.\n");
    } else {
        result = sqrt(number);
        printf("Square root of %.2lf is %.2lf\n", number, result);
    }

    return 0;
}
